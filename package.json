    "autoprefixer": "^10.4.7",
    "axios": "^0.25.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.14.0",
    "dotenv": "^14.3.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "next": "12.0.9",
    "next-connect": "^0.11.1",
    "next-intl": "^2.7.4",
    "node-bigcommerce": "^4.1.0",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-country-flag": "^3.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
